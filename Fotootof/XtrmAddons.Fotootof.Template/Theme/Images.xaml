<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:system="clr-namespace:System;assembly=mscorlib">

	<!-- Icons -->
	<system:String x:Key="Icon24Exit">\Assets\Images\Icons\Buttons\exit-24.png</system:String>


	<Image x:Key="Icon24Back"
		   Source="pack://application:,,,/Assets/Images/Icons/Buttons/back-24.png" />
	
	<Image x:Key="ImageCopy"
		   Source="pack://application:,,,/Assets/Images/Icons/Buttons/copy-24.png" />
	
	<Image x:Key="ImageForward"
		   Source="pack://application:,,,/Assets/Images/Icons/Buttons/forward.png" />
	
	<Image x:Key="ImageUpward"
		   Source="pack://application:,,,/Assets/Images/Icons/Buttons/upward.png" />

</ResourceDictionary>