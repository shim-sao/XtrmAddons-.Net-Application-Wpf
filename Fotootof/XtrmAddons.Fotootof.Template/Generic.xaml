<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:system="clr-namespace:System;assembly=mscorlib">

	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="Generic/Numbers.xaml"/>
		<ResourceDictionary Source="Generic/Fonts.xaml"/>
		<ResourceDictionary Source="Generic/Icons.xaml"/>
		<ResourceDictionary Source="Generic/Converters.xaml"/>

		<ResourceDictionary Source="Generic/Controls/Button/Dictionaries.xaml"/>
	</ResourceDictionary.MergedDictionaries>

	
	
	<system:String x:Key="ImageAbout">Assets\Images\Default\about.jpg</system:String>
	<BitmapImage x:Key="BitmapImageAbout" UriSource="\Assets\Images\Default\about.jpg"></BitmapImage>
	<system:String x:Key="ImageAlbumDefaultBackground">Assets\Images\Default\album-background.png</system:String>
    <system:String x:Key="ImageAlbumDefaultPreview">Assets\Images\Default\album-preview.png</system:String>
	<system:String x:Key="ImageAlbumDefaultThumbnail">Assets\Images\Default\album-thumbnail.png</system:String>
	
	

	<system:String x:Key="AlbumsPictureDirectory">albums</system:String>
	<system:String x:Key="CategoriesPictureDirectory">categories</system:String>
	<system:String x:Key="PicturesDirectory">pictures</system:String>

	<system:Double x:Key="DatagridRowHeight">40</system:Double>

</ResourceDictionary>
